
gsettings get org.gnome.desktop.interface gtk-theme 'DarkCold'
gsettings set org.cinnamon.theme name "DarkCold"
gsettings set org.gnome.desktop.wm.preferences theme "DarkCold"
gsettings set org.cinnamon.theme name "DarkCold"
mateconftool-2 -type=string -set /desktop/mate/interface/gtk_theme "DarkCold"

export GTK_THEME=DarkCold
export QT_STYLE_OVERRIDE=gtk
export QT_QPA_PLATFORMTHEME=gtk
export SAL_GTK_USE_PIXMAPPAINT=1
export SAL_USE_VCLPLUGIN=gtk
export MUFFIN_NO_SHADOWS=1
export GTK_OVERLAY_SCROLLING=0
